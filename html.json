    "wtfform-bootstrap": {
        "prefix": "wtf-bootstrap",
        "body": [
            "<form method=\"post\">",
            "    {{ form.hidden_tag() }}",
            "    <div class=\"form-group\">",
            "        {{ form.$1.label }}",
            "        {% if form.$1.errors %}",
            "        {{ form.$1(class=\"form-control is-invalid\") }}",
            "        <div class=\"invalid-feeback\">",
            "            {% for error in form.$1.errors %}",
            "            <span>{{ error }}</span>",
            "            {% endfor %}",
            "        </div>",
            "        {% else%}",
            "        {{ form.$1(class='form-control')}}",
            "        {% endif %}",
            "    </div>",
            "",
            "    <div class=\"form-group\">",
            "        {{ form.$2.label }}",
            "        {% if form.$2.errors %}",
            "        {{ form.$2(class=\"form-control is-invalid\") }}",
            "        <div class=\"invalid-feeback\">",
            "            {% for error in form.$2.errors %}",
            "            <span>{{ error }}</span>",
            "            {% endfor %}",
            "        </div>",
            "        {% else%}",
            "        {{ form.$2(class='form-control')}}",
            "        {% endif %}",
            "    </div>",
            "",
            "    {{ form.submit }}",
            "</form>"
        ],
        "description": "wtfform-bootstrap"
    },
