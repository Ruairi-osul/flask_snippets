    "title with jinja2 template": {
        "prefix": "jtitle",
        "body": [
            "<title>",
            "    {% if title %}",
            "    {{ title }} | $1",
            "    {% else %}",
            "    $1",
            "    {% endif %}",
            "</title>"
        ],
        "description": "title with jinja2 template"
    },
    "jnav-dark": {
        "prefix": "jnav-dark",
        "body": [
            "<nav class=\"navbar navbar-expand-sm navbar-dark bg-dark mb-5\">",
            "    <!-- Brand -->",
            "    <a class=\"navbar-brand\" href=\"{{url_for('$1') }}\">$2</a>",
            "    <button class=\"navbar-toggler d-lg-none\" type=\"button\" data-toggle=\"collapse\" data-target=\"#collapsibleNavId\"",
            "        aria-controls=\"collapsibleNavId\" aria-expanded=\"false\" aria-label=\"Toggle navigation\">",
            "        <span class=\"navbar-toggler-icon\"></span>",
            "    </button>",
            "    <div class=\"collapse navbar-collapse\" id=\"collapsibleNavId\">",
            "        <!-- Left -->",
            "        <ul class=\"navbar-nav mr-auto mt-2 mt-lg-0\">",
            "            <li class=\"nav-item\">",
            "                <a class=\"nav-link\" href=\"{{ url_for('$3') }}\">$4</a>",
            "            </li>",
            "            <li class=\"nav-item\">",
            "                <a class=\"nav-link\" href=\"{{ url_for('$5') }}\">$6</a>",
            "            </li>",
            "        </ul>",
            "        <!-- Right -->",
            "        <ul class=\"navbar-nav ml-auto mt-2 mt-lg-0\">",
            "            <li class=\"nav-item\">",
            "                <a class=\"nav-link\" href=\"{{ url_for('$7') }}\">$8</a>",
            "            </li>",
            "            <li class=\"nav-item\">",
            "                <a class=\"nav-link\" href=\"{{ url_for('$9') }}\">$10</a>",
            "            </li>",
            "        </ul>",
            "    </div>",
            "</nav>"
        ],
        "description": "jnav-dark"
    },
    "jinline-full": {
        "prefix": "jinline-form-full",
        "body": [
            "<h3 class=\"card-header mb-2 border-bottom bg-dark\">$1</h3>",
            "<form method=\"POST\">",
            "    {{ form.hidden_tag() }} ",
            "    <div class=\"form-row\">",
            "        <div class=\"col-xs-12 col-md-6 my-3 form-group\">",
            "            {{ form.$2.label }}",
            "            {% if form.$2.errors %}",
            "            {{ form.$2(class=\"form-control is-invalid\", placeholder=\"$3\")}}",
            "",
            "            {% for error in form.$2.errors %}",
            "            <div class=\"invalid-feedback\">",
            "                <span> {{error}}</span>",
            "            </div>",
            "            {% endfor %}",
            "            {% else %}",
            "            {{ form.$2(class=\"form-control\", placeholder=\"$3\") }}",
            "            {% endif %}",
            "        </div>",
            "        <div class=\"col-xs-12 col-md-6 my-3 form-group\">",
            "            {{ form.$4.label}}",
            "            {% if form.$4.errors %}",
            "            {{ form.$4(class=\"form-control is-invalid\", placeholder=\"$5\")}}",
            "",
            "            {% for error in form.$4.errors %}",
            "            <div class=\"invalid-feedback\">",
            "                <span> {{error}}</span>",
            "            </div>",
            "            {% endfor %}",
            "            {% else %}",
            "            {{ form.$4(class=\"form-control\", placeholder=\"$5\") }}",
            "            {% endif %}",
            "        </div>",
            "    </div>",
            "    <div class=\"form-row\">",
            "        <div class=\"col-xs-12 col-md-6 my-3 form-group\">",
            "            {{ form.$6.label }}",
            "            {% if form.$6.errors %}",
            "            {{ form.$6(class=\"form-control is-invalid\", placeholder=\"$7\")}}",
            "",
            "            {% for error in form.$6.errors %}",
            "            <div class=\"invalid-feedback\">",
            "                <span> {{error}}</span>",
            "            </div>",
            "            {% endfor %}",
            "            {% else %}",
            "            {{ form.$6(class=\"form-control\", placeholder=\"$7\") }}",
            "            {% endif %}",
            "        </div>",
            "        <div class=\"col-xs-12 col-md-6 my-3 form-group\">",
            "            {{ form.$8.label }}",
            "            {% if form.$8.errors %}",
            "            {{ form.$8(class=\"form-control is-invalid\", placeholder=\"$9\")}}",
            "",
            "            {% for error in form.$8.errors %}",
            "            <div class=\"invalid-feedback\">",
            "                <span> {{error}}</span>",
            "            </div>",
            "            {% endfor %}",
            "            {% else %}",
            "            {{ form.$8(class=\"form-control\", placeholder=\"$9\") }}",
            "            {% endif %}",
            "        </div>",
            "    </div>",
            "",
            "    <div class=\"form-row\">",
            "        <div class=\"col-xs-12\">",
            "            {{ form.submit(class='btn btn-lg btn-primary') }}",
            "        </div>",
            "    </div>",
            "</form>"
        ],
        "description": "jinline-full"
    }
